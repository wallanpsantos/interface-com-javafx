<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="gui.controller.ComboBoxViewController">
	<children>
		<ComboBox fx:id="comboBoxPessoaModelEntity" layoutX="30.0"
			layoutY="49.0" onAction="#onComboBoxPessoaSelecionada"
			prefWidth="150.0" promptText="Selecione uma pessoa" />
		<Button fx:id="exibirTodosButton" layoutX="263.0"
			layoutY="49.0" mnemonicParsing="false"
			onAction="#onExibirTodosButton" text="Exibir todos" />
	</children>
</AnchorPane>
